<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend_adapat" inherit_id="web.assets_frontend">
    
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/css" href="/payment_adapay/static/src/css/adapay_payment.css"/>
        </xpath>

        <xpath expr="script[last()]" position="after">
